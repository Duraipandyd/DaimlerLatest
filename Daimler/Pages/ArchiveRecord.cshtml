    <script type="text/javascript">
        debugger;
        window.onload = function () {
            removeClass()
        }
        function removeClass() {
            document.getElementById("Dashboard").classList.remove("active");
            document.getElementById("duty_payment_request").classList.remove("active");
            document.getElementById("uploadisc").classList.remove("active");
            document.getElementById("uploadidt").classList.remove("active");
            document.getElementById("archive_records").classList.add("active");
        }

        function myfilterfunction ()
        {
            debugger;
            var dprnumber = $('#dpr_number').val();
            var dprdate = $('#dpr_date').val();
            var boenumber = $('#boe_number').val();
            var boedate = $('#boe_date').val();
            $.ajax({
                            type: 'GET',
                            datatype: "json",
                            traditional: true,
                data: { 'dprnumber': dprnumber, 'dprdate': dprdate, 'boenumber': boenumber, 'boedate': boedate },
            url: '@Url.Page("ArchiveRecord", "archive", null)',
            
        });
        


    }
    </script>

